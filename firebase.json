{
  "hosting": [
    {
      "site":"predyc-empresa",
      "source": ".",
      "frameworksBackend": {}
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "firestore": {
      "port": 8080,
      "host": "localhost",
      "rules": "firestore.rules",
      "seed": "database/seed.json"
    },
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "functions": {
      "port": 5001
    },
    "auth": {
      "host": "localhost",
      "port": 9099
    },
    "hosting": {
      "port": 4200
    },
    "pubsub": {
      "port": 8085
    },
    "storage": {
      "port": 9199,
      "rules": "storage.rules"
    },
    "singleProjectMode": true
  },
  "functions": [
    {
      "source": "projects/functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ],
      "timeouts": {
        "updateDataEnterpriseUsage": "540s",
        "updateDataAllEnterprisesUsage": "540s",
        "updateDataAllEnterprisesUsageSchedule": "540s",
        "updateDataEnterpriseRhythm": "540s",
        "updateDataAllEnterprisesRhythm": "540s",
        "updateDataAllEnterprisesRhythmSchedule": "540s"
      },
      "memory": {
        "updateDataEnterpriseUsage": "2GB",
        "updateDataAllEnterprisesUsage": "2GB",
        "updateDataAllEnterprisesUsageSchedule": "2GB",
        "updateDataEnterpriseRhythm": "2GB",
        "updateDataAllEnterprisesRhythm": "2GB",
        "updateDataAllEnterprisesRhythmSchedule": "2GB"
      }
    }
  ],
  "storage": {
    "rules": "storage.rules"
  }
}
